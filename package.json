{
	"name": "teg-parser",
	"version": "0.11.1",
	"author": "Tanishq Kancharla <tanishqkancharla3@gmail.com>",
	"description": "A tiny parser framework",
	"keywords": [
		"parser",
		"PEG",
		"parser expression grammer",
		"toolkit",
		"types"
	],
	"license": "MIT",
	"exports": {
		"./testParser": "./dist/testParser.js",
		".": "./dist/index.js"
	},
	"typesVersions": {
		"*": {
			"index": [
				"./dist/index.d.ts"
			],
			"testParser": [
				"./dist/testParser.d.ts"
			]
		}
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist/*"
	],
	"sideEffects": false,
	"repository": {
		"url": "https://github.com/tanishqkancharla/teg"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"build": "tsc",
		"clean": "rm -rf dist/*",
		"watch": "tsc --watch",
		"release": "npm run clean; npm run build; npm publish",
		"test": "jest --verbose",
		"bundlesize": "esbuild src/index.ts --bundle --minify --outfile=out.js ; rm out.js"
	},
	"devDependencies": {
		"@types/jest": "^27.5.0",
		"esbuild": "^0.15.1",
		"jest": "^28.1.0",
		"ts-jest": "^28.0.5",
		"typescript": "^4.6.4"
	}
}
